#include <stdio.h>
int factorial(int a){
	int resultado=1;
	for(int i=1;i<a;i++){
		resultado*=i;
	}
	return resultado;
}

int main(){
	int numero;
	printf("ingrese un numero para que la funcion halle su factorial: ");
	scanf("%d",&numero);
	int resultado=factorial(numero);
	printf("\nel resultado es: %d",resultado);
	
	
	return 0;
}
