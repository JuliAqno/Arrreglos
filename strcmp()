#include <stdio.h>
#include <string.h>


int main() {
	char palabra1[100];
	char palabra2[100];
	printf("ingrese dos palabras tal que el programa compare si tiene los mismos caracteres\n");
	scanf("%s %s",palabra1,palabra2);
	if(strcmp(palabra1,palabra2) ==0){
		printf("\nlas palabras tienen los mismos caracteres");
	}else{
		if(strcmp(palabra1,palabra2)!=0){
			printf("\nlas palabras no tienen los mismos caracteres");
		}
	}
	
	
	
	return 0;
}

